FROM debian:jessie

MAINTAINER kizi "prozeman@gmail.com"

WORKDIR /root

RUN apt-get update && \
    mkdir -p datasets

ADD en.lhd.inference.2016-04.nt ./
ADD en.sti.debug ./
    
VOLUME ["/root/datasets"]

CMD /bin/bash -c "sleep 1m; mv /root/en.lhd.inference.2016-04.nt /root/datasets/; mv /root/en.sti.debug /root/datasets/; sleep 3m"